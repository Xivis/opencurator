{
  "name": "open_curator",
  "version": "1.0.0",
  "description": "OpenCurator is a decentralized application that allows to curate aggregated Token Curated Registries",
  "main": "index.js",
  "scripts": {
    "deploy:rinkeby": "truffle migrate --network rinkeby --reset",
    "deploy:development": "truffle migrate --network development --reset",
    "deploy:mainnet": "truffle migrate --network mainnet --reset",
    "create-tcr:rinkeby": "truffle exec --network rinkeby ./scripts/create_tcr.js",
    "create-tcr:development": "truffle exec --network ganache ./scripts/create_tcr.js",
    "create-tcr:mainnet": "truffle exec --network mainnet ./scripts/create_tcr.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xivis/opencurator"
  },
  "keywords": [
    "zeppelinos",
    "openzeppelin",
    "truffle",
    "react"
  ],
  "author": "Xivis Team <info@xivis.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xivis/opencurator/issues"
  },
  "homepage": "https://github.com/Xivis/opencurator#readme",
  "dependencies": {
    "dotenv": "^6.2.0",
    "openzeppelin-eth": "^2.1.3",
    "truffle": "5.0.4",
    "solc": "0.4.15",
    "truffle-hdwallet-provider": "^1.0.3",
    "zos-lib": "2.2.0"
  }
}
